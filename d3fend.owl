    <d3f:spoofs rdf:resource="#IntranetNetwork"/>
    <rdfs:label>Integrated Honeynet</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DecoyEnvironment"/>
    <d3f:definition>The practice of setting decoys in a production environment to entice interaction from attackers.</d3f:definition>
