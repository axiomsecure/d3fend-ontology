  <rdf:Description rdf:about="#MessageEncryption">
    <d3f:definition>Encrypting a message body using a cryptographic key.</d3f:definition>
    <rdf:type rdf:resource="#MessageHardening"/>
    <rdfs:subClassOf rdf:nodeID="Nbe78f56cb0ad4a3d8b2709c93c2cd6f1"/>
    <rdfs:label>Message Encryption</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
