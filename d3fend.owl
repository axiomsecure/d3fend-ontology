    <rdfs:subClassOf rdf:resource="#Server"/>
    <d3f:may-run rdf:resource="#OperatingSystem"/>
    <rdfs:isDefinedBy rdf:resource="https://dbpedia.org/page/Name_server"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
