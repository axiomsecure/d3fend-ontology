  <rdf:Description rdf:about="#Reference-SecuringWebTransactions">
    <rdfs:label>Reference - Securing Web Transactions</rdfs:label>
    <d3f:kb-author rdf:datatype="http://www.w3.org/2001/XMLSchema#string">William Haag, Murugiah Souppaya, Paul Turner, William C. Barker, Brett Pleasant, Susan Symington</d3f:kb-author>
