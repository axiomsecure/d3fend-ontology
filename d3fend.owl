    <d3f:kb-reference rdf:resource="#Reference_MalwareDetectionUsingLocalComputationalModels_CrowdstrikeInc"/>
    <d3f:kb-reference rdf:resource="#Reference-UseRkillToStopMalwareProcesses-Ghacks.net"/>
