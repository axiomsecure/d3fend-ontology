    <d3f:definition>Analyzing vendor specific branch call recording in order to detect ROP style attacks.</d3f:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:subClassOf rdf:resource="#ProcessAnalysis"/>
  </rdf:Description>
