    <rdfs:subClassOf rdf:nodeID="Nb6d03daa860b479e9c6a7e43be1e18dd"/>
    <d3f:analyzes rdf:resource="#RPCNetworkTraffic"/>
    <d3f:kb-reference rdf:resource="#Reference_CAR-2014-12-001:RemotelyLaunchedExecutablesViaWMI_MITRE"/>
