    <d3f:analyzes rdf:resource="#Process"/>
    <d3f:kb-reference rdf:resource="#Reference_CAR-2013-03-001:Reg.exeCalledFromCommandShell_MITRE"/>
    <rdf:type rdf:resource="#ProcessAnalysis"/>
  </rdf:Description>
