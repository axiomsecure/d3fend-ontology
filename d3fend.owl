  <rdf:Description rdf:about="#DNSTrafficAnalysis">
    <d3f:kb-reference rdf:resource="#Reference_SinkholingBadNetworkDomainsByRegisteringTheBadNetworkDomainsOnTheInternet_PaloAltoNetworksInc"/>
    <rdfs:label>DNS Traffic Analysis</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
