    <rdfs:label>Reference - Detection of Malicious IDNHomoglyph Domains</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <d3f:kb-is-example-of rdf:resource="#HomoglyphDenylisting"/>
