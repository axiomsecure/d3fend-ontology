    <d3f:kb-reference rdf:resource="#Reference%E2%80%93RFC6376%3ADomainKeysIdentifiedMail%28DKIM%29Signatures%E2%80%93IETF"/>
    <d3f:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Authenticating the sender of a message and ensuring message integrity.</d3f:definition>
