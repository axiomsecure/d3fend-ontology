    <d3f:kb-author rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hoi Chang; Mikhail J. Atallah; John R. Rice</d3f:kb-author>
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://patents.google.com/patent/US7287166B1/en?oq=US-7287166-B1</d3f:has-link>
