  <rdf:Description rdf:about="#Reference_IsolationOfApplicationsWithinAVirtualMachine_Bromium,Inc.">
    <d3f:kb-reference-title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Isolation of applications within a virtual machine</d3f:kb-reference-title>
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://patents.google.com/patent/US9921860B1</d3f:has-link>
