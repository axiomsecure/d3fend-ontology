    <rdfs:subClassOf rdf:resource="#NetworkTrafficAnalysis"/>
    <d3f:analyzes rdf:resource="#NetworkTraffic"/>
    <d3f:enables rdf:resource="#Detect"/>
    <rdf:type rdf:resource="#NetworkTrafficAnalysis"/>
    <d3f:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Establishing baseline communities of network hosts and identifying statistically divergent inter-community communication.</d3f:definition>
