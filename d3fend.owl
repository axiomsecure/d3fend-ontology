    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://nsl.cs.columbia.edu/projects/minestrone/papers/Symbiotes.pdf</d3f:has-link>
    <d3f:kb-author>Ang Cui, Salvatore J. Stolfo</d3f:kb-author>
    <d3f:kb-organization>Department of Computer Science Columbia University</d3f:kb-organization>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
