    <rdf:type rdf:resource="#PatentReference"/>
    <d3f:kb-author>David A. Kaplan</d3f:kb-author>
    <d3f:kb-is-example-of rdf:resource="#Hardware-basedProcessIsolation"/>
  </rdf:Description>
