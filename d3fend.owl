    <rdf:type rdf:resource="#AcademicPaperReference"/>
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://web.archive.org/web/20140407025337/http://www.symantec.com/avcenter/reference/Windows_Vista_Security_Model_Analysis.pdf</d3f:has-link>
