    <rdfs:subClassOf rdf:resource="#DecoyObject"/>
    <d3f:enables rdf:resource="#Deceive"/>
    <d3f:definition>Issuing publicly released media to deceive adversaries.</d3f:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
