    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <d3f:kb-reference rdf:resource="#Reference_InferentialExploitAttemptDetection_CrowdstrikeInc"/>
    <d3f:enables rdf:resource="#Detect"/>
    <rdfs:subClassOf rdf:resource="#OperatingSystemMonitoring"/>
    <d3f:analyzes rdf:resource="#ProcessCodeSegment"/>
