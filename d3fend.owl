  <rdf:Description rdf:about="#PerHostDownload-UploadRatioAnalysis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:nodeID="N9b00ce488f614903a3bb1971062a9d10"/>
