  <rdf:Description rdf:about="#BroadcastDomainIsolation">
    <rdf:type rdf:resource="#NetworkIsolation"/>
    <d3f:kb-reference rdf:resource="#Reference_PrivateVirtualLocalAreaNetworkIsolation_CiscoTechnologyInc"/>
    <rdfs:subClassOf rdf:resource="#NetworkIsolation"/>
    <d3f:filters rdf:resource="#LocalAreaNetworkTraffic"/>
    <rdfs:label>Broadcast Domain Isolation</rdfs:label>
    <d3f:kb-reference rdf:resource="#Reference_BroadcastIsolationAndLevel3NetworkSwitch_HewlettPackardEnterpriseDevelopmentLP"/>
