  <rdf:Description rdf:about="#Reference_CAR-2019-04-002:GenericRegsvr32_MITRE">
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://car.mitre.org/analytics/CAR-2019-04-002/</d3f:has-link>
