  <rdf:Description rdf:about="#Reference-CertificateTransparency">
    <d3f:kb-author>Google</d3f:kb-author>
    <d3f:kb-organization>Google</d3f:kb-organization>
    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.certificate-transparency.org/</d3f:has-link>
