  <rdf:Description rdf:about="#DNSServer">
    <d3f:contains rdf:resource="#Application"/>
    <d3f:runs rdf:resource="#OperatingSystem"/>
    <rdfs:label>DNS Server</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
