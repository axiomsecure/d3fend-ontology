  <rdf:Description rdf:about="#FirmwareEmbeddedMonitoringCode">
    <rdf:type rdf:nodeID="Nba7db737dc3740e288d6fc0307cafc5e"/>
    <rdf:type rdf:nodeID="Na3a668121d554ec99f335d28443c176c"/>
