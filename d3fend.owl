    <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://patents.google.com/patent/US20150163109</d3f:has-link>
    <d3f:kb-is-example-of rdf:resource="#RPCTrafficAnalysis"/>
    <d3f:kb-reference-title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">RPC call interception</d3f:kb-reference-title>
