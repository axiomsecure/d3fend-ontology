    <d3f:kb-reference rdf:resource="#Reference_DecoyPersonasForSafeguardingOnlineIdentityUsingDeception_"/>
    <d3f:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Establishing a fake online identity to misdirect, deceive, and or interact with adversaries.</d3f:definition>
    <rdfs:subClassOf rdf:resource="#DecoyObject"/>
    <rdfs:subClassOf rdf:nodeID="Nb4b98c3286494011900aa688cd181cd3"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
