  <rdf:Description rdf:about="#Reference_CAR-2016-03-002:CreateRemoteProcessViaWMIC_MITRE_Other">
    <rdfs:label>Reference - CAR-2016-03-002: Create Remote Process via WMIC - MITRE</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
