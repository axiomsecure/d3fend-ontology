  <rdf:Description rdf:about="#ProcessTermination">
    <d3f:definition>Terminating a running application process on a computer system.</d3f:definition>
    <rdfs:label>Process Termination</rdfs:label>
    <d3f:kb-reference rdf:resource="#Reference-UseRkillToStopMalwareProcesses-Ghacks.net"/>
    <rdfs:subClassOf rdf:resource="#ProcessEviction"/>
    <rdfs:subClassOf rdf:nodeID="Ne045ba44864745d49451b89c499a3563"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
