    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <d3f:blocks rdf:resource="#OutboundInternetDNSLookupTraffic"/>
    <d3f:blocks rdf:resource="#DNSNetworkTraffic"/>
    <rdfs:label>Reverse Resolution IP Denylisting</rdfs:label>
  </rdf:Description>
