  <rdf:Description rdf:about="#InboundSessionVolumeAnalysis">
    <rdf:type rdf:resource="#NetworkTrafficAnalysis"/>
    <d3f:kb-reference rdf:resource="#Reference-MethodAndSystemForUDPFloodAttackDetection-RioreyLLC"/>
    <d3f:definition>Analyzing inbound network session or connection attempt volume.</d3f:definition>
