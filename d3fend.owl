    <d3f:enables rdf:resource="#Detect"/>
    <rdf:type rdf:resource="#ProcessAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <d3f:definition>Analyzing the execution of a script to detect unauthorized user activity.</d3f:definition>
    <d3f:kb-reference rdf:resource="#Reference_DetectingScript-basedMalware_CrowdstrikeInc"/>
