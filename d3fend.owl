    <d3f:contains rdf:resource="#Identifier"/>
    <rdfs:subClassOf rdf:resource="#DigitalArtifact"/>
    <d3f:contains rdf:resource="#PublicKey"/>
    <rdfs:label>Certificate</rdfs:label>
    <skos:altLabel>Public Key Certificate</skos:altLabel>
  </rdf:Description>
