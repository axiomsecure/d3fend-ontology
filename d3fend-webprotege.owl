    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_ApproachesForSecuringAnInternetEndpointUsingFine-grainedOperatingSystemVirtualization_Bromium,Inc. -->

    <owl:NamedIndividual rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_ApproachesForSecuringAnInternetEndpointUsingFine-grainedOperatingSystemVirtualization_Bromium,Inc.">
        <rdf:type rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#PatentReference"/>
        <d3f:kb-is-example-of rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#Hardware-BasedProcessIsolation"/>
