        <rdfs:seeAlso rdf:resource="http://dbpedia.org/page/Enterprise_software"/>
    </owl:Class>
    


    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#UserBehavior -->

    <owl:Class rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#UserBehavior">
        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalArtifact"/>
