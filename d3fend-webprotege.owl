        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An action performed by a user. Executing commands, granting permissions, and accessing resources are examples of user actions.</rdfs:comment>
        <rdfs:label>User Action</rdfs:label>
    </owl:Class>
    


    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#UserApplication -->

    <owl:Class rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#UserApplication">
