    </owl:NamedIndividual>
    


    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_AutomaticallyGeneratingRulesForConnectionSecurity_Microsoft -->

    <owl:NamedIndividual rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_AutomaticallyGeneratingRulesForConnectionSecurity_Microsoft">
        <rdf:type rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#PatentReference"/>
        <d3f:kb-is-example-of rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#InboundTrafficFiltering"/>
