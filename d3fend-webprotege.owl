    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_CAR-2013-02-012:UserLoggedInToMultipleHosts_MITRE -->

    <owl:NamedIndividual rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_CAR-2013-02-012:UserLoggedInToMultipleHosts_MITRE">
        <rdf:type rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#ExternalKnowledgeBase"/>
        <d3f:kb-is-example-of rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#AuthenticationEventThresholding"/>
        <d3f:kb-is-example-of rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#AuthorizationEventThresholding"/>
        <d3f:has-link rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://car.mitre.org/analytics/CAR-2013-02-012/</d3f:has-link>
