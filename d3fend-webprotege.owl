    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_CAR-2013-02-008:SimultaneousLoginsOnAHost_MITRE -->

    <owl:NamedIndividual rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#Reference_CAR-2013-02-008:SimultaneousLoginsOnAHost_MITRE">
        <rdf:type rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#ExternalKnowledgeBase"/>
