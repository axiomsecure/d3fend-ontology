    rdfs:label "User Account" ;
    rdfs:comment "A user account allows a user to authenticate to a system and potentially to receive authorization to access resources provided by or connected to that system; however, authentication does not imply authorization. To log into an account, a user is typically required to authenticate oneself with a password or other credentials for the purposes of accounting, security, logging, and resource management."^^xsd:string ;
    rdfs:isDefinedBy <https://en.wikipedia.org/wiki/User_(computing)#User_account> ;
    rdfs:seeAlso <http://dbpedia.org/page/User_account> ;
    rdfs:subClassOf ns1:DigitalArtifact .

ns1:ApplicationConfiguration a owl:Class ;
    rdfs:label "Application Configuration" ;
    rdfs:comment "Information used to configure the parameters and initial settings for an application."^^xsd:string ;
    rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/05739724-n> ;
    rdfs:subClassOf ns1:ConfigurationItem .

ns1:NetworkTraffic a owl:Class ;
    rdfs:label "Network Traffic" ;
    ns1:may-contain ns1:DomainName ;
    ns1:originates-from ns1:PhysicalLocation ;
    rdfs:comment "Network traffic or data traffic is the data, or alternatively the amount of data, moving across a network at a given point of time.  Network data in computer networks is mostly encapsulated in network packets, which provide the load in the network."^^xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Network_traffic> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:may-contain ;
            owl:someValuesFrom ns1:DomainName ],
        [ a owl:Restriction ;
            owl:onProperty ns1:originates-from ;
            owl:someValuesFrom ns1:PhysicalLocation ],
        ns1:DigitalArtifact ;
    skos:altLabel "Data Traffic"^^xsd:string .

ns1:Process a owl:Class ;
    rdfs:label "Process" ;
    ns1:contains ns1:ProcessImage ;
    ns1:process-image-path ns1:ExecutableBinary ;
    ns1:process-user ns1:UserAccount ;
    rdfs:comment "A process is an instance of a computer program that is being executed. It contains the program code and its current activity. Depending on the operating system (OS), a process may be made up of multiple threads of execution that execute instructions concurrently. A computer program is a passive collection of instructions, while a process is the actual execution of those instructions. Several processes may be associated with the same program; for example, opening up several instances of the same program often means more than one process is being executed."^^xsd:string ;
    rdfs:isDefinedBy <http://dbpedia.org/page/Process_(computing)> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:processIdentifier ;
            owl:someValuesFrom xsd:integer ],
        [ a owl:Restriction ;
            owl:onProperty ns1:processSecurityContext ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty ns1:processEnvironmentalVariables ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty ns1:contains ;
            owl:someValuesFrom ns1:ProcessImage ],
        [ a owl:Restriction ;
            owl:onProperty ns1:processCommandLineArgs ;
            owl:someValuesFrom xsd:string ],
        [ a owl:Restriction ;
            owl:onProperty ns1:process-image-path ;
            owl:someValuesFrom ns1:ExecutableBinary ],
        [ a owl:Restriction ;
            owl:onProperty ns1:process-user ;
            owl:someValuesFrom ns1:UserAccount ],
        ns1:DigitalArtifact .

ns1:may-modify a owl:ObjectProperty ;
    rdfs:label "may-modify" ;
    rdfs:subPropertyOf ns1:may-be-associated-with .

ns1:CredentialAccess a ns1:OffensiveTactic,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Credential Access" ;
    ns1:display-order 6 ;
    rdfs:subClassOf ns1:OffensiveTactic .

ns1:ExternalKnowledgeBase a owl:Class ;
    rdfs:label "External Knowledge Base"^^xsd:string ;
    ns1:preferred-label "External Knowledge Base"^^xsd:string ;
    rdfs:subClassOf ns1:TechniqueReference .

ns1:MarketingMaterial a ns1:ReferenceType,
        owl:NamedIndividual ;
    rdfs:label "Marketing Material" .

ns1:analyzes a owl:ObjectProperty ;
    rdfs:label "analyzes" ;
    rdfs:subPropertyOf ns1:associated-with .

ns1:accesses a owl:ObjectProperty ;
    rdfs:label "accesses" ;
    rdfs:comment "An entity (process or user) accesses a resource.  Reads and writes are specific cases of accesses."^^xsd:string ;
    rdfs:range ns1:NetworkResource ;
    rdfs:subPropertyOf ns1:associated-with,
        ns1:may-access .

ns1:UserManual a ns1:ReferenceType,
        owl:NamedIndividual ;
    rdfs:label "User Manual" .

ns1:associated-with a owl:ObjectProperty ;
    rdfs:label "associated-with" ;
    rdfs:subPropertyOf ns1:may-be-associated-with ;
    skos:altLabel "associatedWith"^^xsd:string .

ns1:Resource a owl:Class ;
    rdfs:comment "In computing, a system resource, or simply resource, is any physical or virtual component of limited availability within a computer system. Every device connected to a computer system is a resource. Every internal system component is a resource. Virtual system resources include files (concretely file handles), network connections (concretely network sockets), and memory areas. Managing resources is referred to as resource management, and includes both preventing resource leaks (releasing a resource when a process has finished using it) and dealing with resource contention (when multiple processes wish to access a limited resource)."^^xsd:string ;
    rdfs:isDefinedBy <http://dbpedia.org/page/System_resource> ;
    rdfs:subClassOf ns1:DigitalArtifact .

ns1:URL a owl:Class ;
    rdfs:label "URL" ;
    ns1:addresses ns1:Resource ;
    rdfs:comment "A Uniform Resource Locator (URL), commonly informally termed a web address (a term which is not defined identically) is a reference to a web resource that specifies its location on a computer network and a mechanism for retrieving it.A URL is a specific type of Uniform Resource Identifier (URI), although many people use the two terms interchangeably. A URL implies the means to access an indicated resource, which is not true of every URI. URLs occur most commonly to reference web pages (http), but are also used for file transfer (ftp), email (mailto), database access (JDBC), and many other applications."^^xsd:string ;
    rdfs:isDefinedBy <http://dbpedia.org/page/Uniform_Resource_Locator> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:addresses ;
            owl:someValuesFrom ns1:Resource ],
        ns1:DigitalArtifact,
        ns1:Identifier ;
    skos:altLabel "Uniform Resource Locator"^^xsd:string .

ns1:Detect a ns1:DefensiveTactic,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Detect" ;
    ns1:definition "Detection endeavors to identify unauthorized activity on computer networks."^^xsd:string ;
    ns1:display-order 1 ;
    rdfs:subClassOf ns1:DefensiveTactic .

ns1:ExecutableFile a owl:Class ;
    rdfs:label "Executable File"^^xsd:string ;
    ns1:may-contain ns1:File,
        ns1:URL ;
    rdfs:comment "In computing, executable code or an executable file or executable program, sometimes simply an executable, causes a computer \"to perform indicated tasks according to encoded instructions,\" as opposed to a data file that must be parsed by a program to be meaningful. These instructions are traditionally machine code instructions for a physical CPU. However, in a more general sense, a file containing instructions (such as bytecode) for a software interpreter may also be considered executable; even a scripting language source file may therefore be considered executable in this sense. The exact interpretation depends upon the use; while the term often refers only to machine code files, in the context of protection against computer viruses all files which cause potentially hazardous instruction"^^xsd:string ;
    rdfs:isDefinedBy <http://dbpedia.org/page/Executable> ;
    rdfs:subClassOf ns1:File ;
    skos:altLabel "Executable"^^xsd:string .

ns1:real-time-analytic a ns1:AnalyticLatency,
        owl:NamedIndividual ;
    rdfs:label "real-time-analytic" .

ns1:DigitalArtifact a owl:Class ;
    rdfs:label "Digital Artifact" ;
    rdfs:comment "An information-bearing artifact that is, or is encoded to be used with, a digital computer system."^^xsd:string ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Digital_artifactual_value>,
        <https://en.wikipedia.org/wiki/Virtual_artifact> ;
    rdfs:subClassOf ns1:Artifact,
        ns1:DigitalObject .

ns1:produces a owl:ObjectProperty ;
    rdfs:label "produces" ;
    rdfs:comment "The subject entity or process produces data objects, same as producer/consumer relationship."^^xsd:string ;
    rdfs:domain ns1:Technique ;
    rdfs:range ns1:DigitalArtifact ;
    rdfs:subPropertyOf ns1:associated-with,
        ns1:may-produce .

ns1:non-real-time-analytic a ns1:AnalyticLatency,
        owl:NamedIndividual ;
    rdfs:label "non-real-time-analytic" .

ns1:PrivilegeEscalation a ns1:OffensiveTactic,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Privilege Escalation" ;
    ns1:display-order 4 ;
    rdfs:subClassOf ns1:OffensiveTactic .

ns1:Persistence a ns1:OffensiveTactic,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Persistence"^^xsd:string ;
    ns1:display-order 3 ;
    rdfs:subClassOf ns1:OffensiveTactic .

ns1:modifies a owl:ObjectProperty ;
    rdfs:label "modifies"^^xsd:string ;
    rdfs:comment "Causes a change to an object, Used in sense that the data held by the object changed."^^xsd:string ;
    rdfs:subPropertyOf ns1:associated-with,
        ns1:may-modify .

ns1:PatentReference a owl:Class ;
    rdfs:label "Patent Reference" ;
    ns1:preferred-label "Patent"^^xsd:string ;
    rdfs:subClassOf ns1:TechniqueReference .

ns1:File a owl:Class ;
    rdfs:label "File" ;
    ns1:may-contain ns1:File,
        ns1:URL ;
    rdfs:comment "A file maintained in computer-readable form."^^xsd:string ;
    rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/06521201-n> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:may-contain ;
            owl:someValuesFrom ns1:File ],
        [ a owl:Restriction ;
            owl:onProperty ns1:may-contain ;
            owl:someValuesFrom ns1:URL ],
        ns1:DigitalArtifact,
        ns1:Resource .

ns1:NativeSupport a ns1:SupportLevel,
        owl:NamedIndividual ;
    rdfs:label "Native Support" .

ns1:DefenseEvasion a ns1:OffensiveTactic,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Defense Evasion" ;
    ns1:display-order 5 ;
    rdfs:subClassOf ns1:OffensiveTactic .

ns1:OperatingSystem a owl:Class ;
    rdfs:label "Operating System" ;
    ns1:contains ns1:Kernel,
        ns1:SystemServiceApplication ;
    ns1:may-contain ns1:OperatingSystemConfigurationComponent ;
    rdfs:comment "An operating system (OS) is system software that manages computer hardware and software resources and provides common services for computer programs. All computer programs, excluding firmware, require an operating system to function. Time-sharing operating systems schedule tasks for efficient use of the system and may also include accounting software for cost allocation of processor time, mass storage, printing, and other resources."@en ;
    rdfs:seeAlso <http://dbpedia.org/page/Operating_system> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:contains ;
            owl:someValuesFrom ns1:Kernel ],
        [ a owl:Restriction ;
            owl:onProperty ns1:may-contain ;
            owl:someValuesFrom ns1:OperatingSystemConfigurationComponent ],
        [ a owl:Restriction ;
            owl:onProperty ns1:contains ;
            owl:someValuesFrom ns1:SystemServiceApplication ],
        ns1:DigitalArtifact .

ns1:TechniqueAssertion a owl:Class ;
    rdfs:label "Technique Assertion" ;
    rdfs:comment "A claim that a particular d3f:Product contains a d3f:Technique"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:has-citation ;
            owl:someValuesFrom ns1:AnalysisCitation ],
        [ a owl:Restriction ;
            owl:onProperty ns1:has-technique ;
            owl:someValuesFrom ns1:DefensiveTechnique ],
        [ a owl:Restriction ;
            owl:onProperty ns1:has-support-level ;
            owl:someValuesFrom ns1:SupportLevel ],
        [ a owl:Restriction ;
            owl:onProperty ns1:has-product ;
            owl:someValuesFrom ns1:Product ],
        [ a owl:Restriction ;
            owl:onProperty ns1:has-expected-latency ;
            owl:someValuesFrom ns1:Latency ],
        ns1:D3FENDAnalysisThing .

ns1:AnalysisCitation a owl:Class ;
    rdfs:label "Analysis Citation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:has-reference-type ;
            owl:someValuesFrom ns1:ReferenceType ],
        [ a owl:Restriction ;
            owl:onProperty ns1:cited-by ;
            owl:someValuesFrom ns1:TechniqueAssertion ],
        [ a owl:Restriction ;
            owl:onProperty ns1:has-link ;
            owl:someValuesFrom xsd:anyURI ],
        ns1:D3FENDAnalysisThing .

ns1:attack-technique-id a owl:DatatypeProperty ;
    rdfs:label "attack-technique-id" ;
    rdfs:domain ns1:OffensiveTechnique ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf ns1:attack-kb-data-property .

