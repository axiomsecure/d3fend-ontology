        ns1:Reference_CAR-2014-03-001:SMBWriteRequest-NamedPipes_MITRE,
        ns1:Reference_SecuritySystemWithMethodologyForInterprocessCommunicationControl_CheckPointSoftwareTechInc ;
    rdfs:subClassOf [ a owl:Restriction ;
