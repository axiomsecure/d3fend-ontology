    <rdf:Description rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#installs">
        <rdfs:comment>x installs y: An entity x sets up a digital artifact y for subsequent use.  For example, an installation program can install application software.</rdfs:comment>
        <rdfs:label>installs</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://wordnet-rdf.princeton.edu/id/01572394-v"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#interprets">
        <rdfs:label>interprets</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#invokes">
