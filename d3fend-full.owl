        <rdfs:isDefinedBy rdf:resource="http://dbpedia.org/page/Local_area_network"/>
        <rdfs:label>Local Area Network</rdfs:label>
        <skos:altLabel>LAN</skos:altLabel>
    </owl:Class>
    


