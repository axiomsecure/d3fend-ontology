        <rdfs:isDefinedBy rdf:resource="https://en.wikipedia.org/wiki/HTTP_cookie#Session_cookie"/>
        <rdfs:label>Session Cookie</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://dbpedia.org/page/HTTP_cookie"/>
        <skos:altLabel>In-memory Cookie</skos:altLabel>
        <skos:altLabel>Non-persistent Cookie</skos:altLabel>
        <skos:altLabel>Transient Cookie</skos:altLabel>
