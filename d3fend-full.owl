    <!-- http://d3fend.mitre.org/ontologies/d3fend.owl#User -->

    <owl:Class rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#User">
        <rdfs:subClassOf rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalArtifact"/>
