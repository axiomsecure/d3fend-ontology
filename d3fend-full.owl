    <rdf:Description rdf:about="http://d3fend.mitre.org/ontologies/d3fend.owl#One-timePassword">
        <rdfs:label>One-time Password</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://dbpedia.org/page/One-time_password"/>
        <definition>A one-time password is valid for only one user authentication.</definition>
        <use-limits rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#Password"/>
        <enables rdf:resource="http://d3fend.mitre.org/ontologies/d3fend.owl#Harden"/>
